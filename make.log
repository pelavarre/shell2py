shell2py find -maxdepth 1 -type d
import os

def find(top):

    print(top)
    for (dirpath, dirnames, filenames) in os.walk(top):
        depth = 1 + dirpath.count(os.sep)

        if depth > 1:
            continue

        for dirname in dirnames:
            found_dir = os.path.join(dirpath, dirname)

            print(found_dir)


